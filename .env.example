# Kleine Anfragen Crawlify - Environment Variables
# Copy this file to .env and fill in your values

# DIP API Key (required)
# Get your key from: https://dip.bundestag.de/
DIP_API_KEY=your-api-key-here

# Search UI Admin Credentials (optional, defaults shown)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me-in-production

# API Server Settings (optional)
API_HOST=0.0.0.0
API_PORT=8000
